<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	version="4.0">
	<display-name>Servlet_EmployeeSearchApp20_RequestDispater</display-name>
	<welcome-file-list>
		<welcome-file>input.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>error_html</servlet-name>
		<jsp-file>/errorhtml.html</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>error_html</servlet-name>
		<url-pattern>/error_htmlurl</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>error_jsp</servlet-name>
		<jsp-file>/errorjsp.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>error_jsp</servlet-name>
		<url-pattern>/error_jspurl</url-pattern>
	</servlet-mapping>
	
</web-app>